            </StackPanel>
            <StackPanel Height="150" Margin="0">
                <TextBlock x:Name="textBlock29" TextWrapping="Wrap" Text="79" FontSize="66.667" Margin="0,10,0,0" Foreground="#FFEEE641" HorizontalAlignment="Center"/>
                <TextBlock x:Name="textBlock30" TextWrapping="Wrap" Text="Opgaver udført" FontSize="26.667" Foreground="White" HorizontalAlignment="Center"/>
            </StackPanel>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="967" Grid.Row="1" Grid.Column="2" VerticalAlignment="Top" Width="699">
            <TextBlock x:Name="textBlock12" TextWrapping="Wrap" Text="Medarbejderes tid" FontSize="40" Foreground="White" HorizontalAlignment="Center"/>
            <StackPanel Height="150" Margin="0">
                <TextBlock x:Name="textBlock13" TextWrapping="Wrap" Text="79" FontSize="66.667" Margin="0,10,0,0" Foreground="#FFEEE641" HorizontalAlignment="Center"/>
                <TextBlock x:Name="textBlock14" TextWrapping="Wrap" Text="Opgaver udført" FontSize="26.667" Foreground="White" HorizontalAlignment="Center"/>
            </StackPanel>
            <StackPanel Height="150" Margin="0">
                <TextBlock x:Name="textBlock15" TextWrapping="Wrap" Text="79" FontSize="66.667" Margin="0,10,0,0" Foreground="#FFEEE641" HorizontalAlignment="Center"/>
                <TextBlock x:Name="textBlock16" TextWrapping="Wrap" Text="Opgaver udført" FontSize="26.667" Foreground="White" HorizontalAlignment="Center"/>
            </StackPanel>
            <StackPanel Height="150" Margin="0">
                <TextBlock x:Name="textBlock17" TextWrapping="Wrap" Text="79" FontSize="66.667" Margin="0,10,0,0" Foreground="#FFEEE641" HorizontalAlignment="Center"/>
                <TextBlock x:Name="textBlock18" TextWrapping="Wrap" Text="Opgaver udført" FontSize="26.667" Foreground="White" HorizontalAlignment="Center"/>
            </StackPanel>
            <StackPanel Height="150" Margin="0">
                <TextBlock x:Name="textBlock19" TextWrapping="Wrap" Text="79" FontSize="66.667" Margin="0,10,0,0" Foreground="#FFEEE641" HorizontalAlignment="Center"/>
                <TextBlock x:Name="textBlock31" TextWrapping="Wrap" Text="Opgaver udført" FontSize="26.667" Foreground="White" HorizontalAlignment="Center"/>
            </StackPanel>
            <StackPanel Height="150" Margin="0">
                <TextBlock x:Name="textBlock32" TextWrapping="Wrap" Text="79" FontSize="66.667" Margin="0,10,0,0" Foreground="#FFEEE641" HorizontalAlignment="Center"/>
                <TextBlock x:Name="textBlock33" TextWrapping="Wrap" Text="Opgaver udført" FontSize="26.667" Foreground="White" HorizontalAlignment="Center"/>
            </StackPanel>
        </StackPanel>


    </Grid>
</Page>
